{
  "name": "sapindale",
  "description": "Canadiana platform administration",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "PORT=8443 COUCH=https://upholstery.canadiana.ca sapper dev",
    "build": "sapper build -p $PORT && yarn run build:tailwind",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.7.0",
    "compression": "^1.7.4",
    "polka": "^0.5.2",
    "query-string": "^6.8.3",
    "postcss-cli": "^6.1.3",
    "sirv": "^0.4.2",
    "tailwindcss": "^1.1.3"
  },
  "devDependencies": {
    "cypress": "^3.4.1",
    "npm-run-all": "^4.1.5",
    "purgecss": "^1.4.1",
    "sapper": "^0.27.4",
    "svelte": "^3.6.4",
    "svelte-loader": "^2.13.4",
    "webpack": "^4.35.2"
  }
}
